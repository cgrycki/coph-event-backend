{
  "name": "coph-backend",
  "version": "0.1.0",
  "description": "Server for CoPH event creation app.",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "predeploy": "claudia generate-serverless-express-proxy --express-module server",
    "deploy": "npm run predeploy && TMPDIR=/tmp claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1",
    "update": "npm run predeploy && TMPDIR=/tmp claudia update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.uiowa.edu/rlrson/coph-event-backend.git"
  },
  "author": "College of Public Health IT",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.279.1",
    "aws-serverless-express": "^3.2.0",
    "aws-xray-sdk": "^1.3.0",
    "body-parser": "^1.18.3",
    "claudia": "^5.1.0",
    "connect-dynamodb": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "dynamodb": "^1.1.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "helmet": "^3.13.0",
    "jsdoc-to-markdown": "^4.0.1",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "nodemon": "^1.18.3",
    "request-promise": "^4.2.2",
    "simple-oauth2": "^2.2.0"
  }
}
